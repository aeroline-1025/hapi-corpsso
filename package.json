{
  "name": "hapi-passport-saml",
  "version": "2.1.1",
  "description": "A Hapi plugin that wraps passport-saml for SAML SSO (as SP)",
  "main": "lib/index",
  "types": "lib/index",
  "scripts": {
    "start": "npm run build:live",
    "build:live": "nodemon --exec ./node_modules/.bin/ts-node -- ./src/index.ts",
    "build": "tsc -P tsconfig.json",
    "pretest": "npm run build",
    "test": "lab"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "files": [
    "lib"
  ],
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/molekilla/hapi-passport-saml.git"
  },
  "keywords": [
    "saml",
    "sso",
    "hapi"
  ],
  "author": "Rogelio Morrell",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/molekilla/hapi-passport-saml/issues"
  },
  "homepage": "https://github.com/molekilla/hapi-passport-saml#readme",
  "dependencies": {
    "@hapi/lab": "^19.1.0",
    "boom": "^7.3.0",
    "debug": "^2.2.0",
    "passport-saml": "^1.1.0"
  },
  "peerDependencies": {
    "hapi": ">= 17.0.0"
  },
  "devDependencies": {
    "@hapi/code": "^5.3.1",
    "@hapi/hapi": "^18.3.1",
    "@types/hapi": "^15.0.2",
    "@types/node": "^10.14.10",
    "nodemon": "^1.19.1",
    "ts-node": "^7.0.1",
    "typescript": "^2.9.2"
  }
}
